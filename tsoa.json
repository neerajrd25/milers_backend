{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "securityDefinitions": {
      "api_key": {
          "type": "apiKey",
          "name": "access_token",
          "in": "query"
      },
      "tsoa_auth": {
          "type": "oauth2",
          "authorizationUrl": "http://swagger.io/api/oauth/dialog",
          "flow": "implicit"
      }
  },
    "outputDirectory": "public",
    "specVersion": 3
  },
  "ignore": ["**/node_modules/**"]
}
